{
  "type": "module",
  "name": "@hazae41/sha1",
  "version": "1.1.11",
  "description": "SHA-1 adapter for WebAssembly and JS implementations",
  "homepage": "https://github.com/hazae41/sha1",
  "repository": "github:hazae41/sha1",
  "author": "hazae41",
  "license": "MIT",
  "main": "./dist/cjs/src/index.cjs",
  "module": "./dist/esm/src/index.mjs",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "./dist/esm",
    "./dist/cjs",
    "./dist/types"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "test": "node ./dist/test/index.test.mjs",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@hazae41/phobos": "^1.0.10",
    "@hazae41/rimraf": "^1.0.1",
    "@hazae41/symbol-dispose-polyfill": "^1.0.2",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.9.2",
    "rollup": "^4.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-node-externals": "^6.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/src/index.mjs",
      "require": "./dist/cjs/src/index.cjs"
    }
  },
  "keywords": [
    "sha1",
    "sha-1",
    "crypto",
    "hashing",
    "cryptography",
    "adapter",
    "typescript",
    "esmodules"
  ],
  "dependencies": {
    "@hazae41/box": "^1.0.11",
    "@hazae41/option": "^1.0.27",
    "@hazae41/result": "^1.1.11"
  },
  "optionalDependencies": {
    "@hazae41/morax": "^1.1.5",
    "@noble/hashes": "^1.3.2"
  }
}
